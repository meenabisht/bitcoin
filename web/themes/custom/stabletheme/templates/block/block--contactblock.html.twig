{{ kint(content) }}
<div class = "free-main">
  <div class="freeimage" style="background-image: url('{{ file_url(content.field_free_image.widget.0.preview['#uri']) }}');">

  <div class = "container">
    <div class = "freetitle">
      <div class = "free-subtitle">
        {{ content.field_free_subtitle.widget.0.value['#default_value'] }}
      </div>
      <div class = "free-title">
        {{ content.field_free_title.widget.0.value['#default_value'] }}
      </div>
    </div>

    <div class = "contact-form">

      <div class = "contact-wrapper">
        
        <div class = "free-name">
          {{ content.field_full_name }}
        </div>
        <div class = "free-address">
          {{ content.field_address }}
        </div>
        <div class = "free-subject">
          {{ content.field_your_subject }}
        </div>
        </div>

        <div class = "free-question">
          {{ content.field_your_question }}
        </div>
      </div>

      <div class = "send-link">
        {{ content.field_send_link.widget.0.title }}
      </div>

    </div>
  
  </div>
</div>